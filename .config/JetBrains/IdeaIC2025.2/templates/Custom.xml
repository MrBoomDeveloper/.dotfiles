    </context>
  </template>
  <template name="file" value="$VAR$.also { &#10;    java.io.File(&quot;temp.txt&quot;).apply { createNewFile(); writeText(it) } &#10;}" description="" toReformat="false" toShortenFQNames="true" deactivated="true">
